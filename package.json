{
    "name": "vsts-react-widgets",
    "version": "0.0.1",
    "description": "Reusable React components based on office fabric for building VSTS extensions.",
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/mohitbagra/vsts-react"
    },
    "license": "MIT",
    "author": "mohitbagra",
    "scripts": {
        "clean": "rimraf lib dist",
        "cleanall": "npm run clean && rimraf typings node_modules",
        "build:lib": "webpack --display-error-details --progress --colors --output-path ./lib -p",
        "build": "npm run clean && mkdir lib && mkdir dist && npm run build:lib && copy lib\\index.js dist\\vsts-react-widgets.min.js",
        "postinstall": "typings install"
    },
    "devDependencies": {
        "@types/react": "^15.0.16",
        "@types/react-dom": "^0.14.23",
        "@types/jquery": "^2.0.41",
        "webpack": "^2.4.1",
        "webpack-dev-server": "^2.4.2",
        "rimraf": "^2.6.1",
        "sass-loader": "^6.0.3",
        "style-loader": "^0.16.1",
        "css-loader": "^0.28.0",
        "awesome-typescript-loader": "^3.1.2",
        "node-sass": "^4.5.2",
        "typescript": "^2.2.2",
        "typings": "^2.1.0",
        "vss-web-extension-sdk": "^2.115.0",
        "uglifyjs-webpack-plugin": "^0.4.3"
    },
    "dependencies": {
        "office-ui-fabric-react": "^2.20.2"
    }
}